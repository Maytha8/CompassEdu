{
  "opts": {
    "template": "node_modules/better-docs",
    "readme": "README.md",
    "recurse": true
  },
  "source": {
    "include": ["."],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "templates": {
    "better-docs": {
        "name": "CompassEdu Documentation"
    }
  },
  "plugins": [
    "plugins/markdown"
  ]
}
