{
  "opts": {
    "template": "node_modules/better-docs",
    "readme": "README.md",
    "recurse": true,
    "tutorials": "./tutorials"
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "templates": {
    "better-docs": {
        "name": "CompassEdu Documentation"
    }
  },
  "plugins": [
    "plugins/markdown"
  ]
}
