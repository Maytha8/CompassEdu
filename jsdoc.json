{
  "opts": {
    "template": "node_modules/better-docs",
    "readme": "jsdoc-index.md",
    "recurse": true,
    "tutorials": "docs"
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "templates": {
    "better-docs": {
        "name": "CompassEdu Documentation",
        "title": "CompassEdu Documentation",
        "hideGenerator": true,
        "navLinks": [
          {
            "label": "GitHub",
            "href": "https://github.com/Maytha8/CompassEdu"
          }
        ]
    }
  },
  "plugins": [
    "plugins/markdown"
  ]
}
